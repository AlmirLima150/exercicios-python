notas = []

while True:
    num1 = int(input("digite a nota do aluno: "))
    num2 = int(input("digite a nota do aluno: "))

    media = (num1 + num2) / 2

    notas.append(media)

    if media >= 7:
        print("Aluno aprovado")
    elif media >= 4:
        print("Aluno em recuperação")
    else:
        print("Aluno Reprovado")

    outro_aluno = input("tem outro aluno? ")
    if outro_aluno != "nao":
        continue
    else:
        break

print(notas)
